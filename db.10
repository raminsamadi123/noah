;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	domain.se. root.domain.se. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	raminserver.
0	IN	PTR	raminserver.domain.se.
104	IN	PTR	test.domain.se.
